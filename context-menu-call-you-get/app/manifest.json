{
  "name": "run you-get",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
      "scripts": ["background.js"]
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["getContentLink.js"]
      }
  ],
  "browser_action": {
      "default_title": "run you-get"
  },
  "description": "run you-get",
  "icons": {
      "128": "icon-128.png"
  },
  "permissions": [
      "contextMenus",
      "nativeMessaging",
      "tabs",  
      "http://xxx/*"  
  ]
}
